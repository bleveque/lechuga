{
	// required fields
	"manifest_version": 2,
	"name": "Lechuga",
	"version": "1.0.0",

	// "default_locale": "en",
	"description": "Monitor Chrome usage",
	"icons": {},

	// permissions
	"permissions": [
		"processes",
	]

	"browser_action": {
		"default_icon": {
			"19": "images/squirrel19.jpg",
			"38": "images/squirrel38.jpg",
			"64": "images/squirrel.jpg"
		},
		"default_title": "Lechuga",
		"default_popup": "popup.html"
	},
}